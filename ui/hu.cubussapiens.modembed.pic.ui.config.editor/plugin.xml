<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            id="hu.gb.embedded.pic.ui"
            name="PIC embedded">
      </category>
      <wizard
            category="hu.gb.embedded.pic.ui"
            class="hu.cubussapiens.modembed.pic.ui.config.editor.wizards.NewPICConfiguration"
            icon="icons/chip_config.png"
            id="hu.gb.embedded.pic.ui.config.editor.wizards.NewPICConfiguration"
            name="New PIC Configuration"
            project="false">
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="hu.cubussapiens.modembed.pic.ui.config.editor.PICConfigEditorPart"
            default="true"
            extensions="config"
            icon="icons/chip_config.png"
            id="hu.gb.embedded.pic.ui.config.editor"
            name="PIC configuration editor">
      </editor>
   </extension>

</plugin>
