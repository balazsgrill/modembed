<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="hu.cubussapiens.modembed.ui.project.extensions" name="Project extensions" schema="schema/hu.cubussapiens.modembed.ui.project.extensions.exsd"/>
   <extension
         id="nature"
         point="org.eclipse.core.resources.natures">
      <runtime>
         <run
               class="hu.cubussapiens.modembed.ui.ProjectNature">
         </run>
      </runtime>
      <builder
            id="hu.cubussapiens.modembed.ui.masmbuilder">
      </builder>
   </extension>
   <extension
         point="org.eclipse.ui.ide.projectNatureImages">
      <image
            icon="icons/chip.png"
            id="hu.cubussapiens.modembed.ui.nature.icon"
            natureId="hu.cubussapiens.modembed.ui.nature">
      </image>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <wizard
            category="hu.cubussapiens.modembed.ui.wizards"
            class="hu.cubussapiens.modembed.ui.wizards.NewMODembedProjectWizard"
            icon="icons/project_chip.png"
            id="hu.cubussapiens.modembed.ui.newProject"
            name="New MODembed project"
            project="true">
      </wizard>
      <category
            id="hu.cubussapiens.modembed.ui.wizards"
            name="MODembed">
      </category>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="hu.cubussapiens.modembed.ui.project.settingsEditor.ProjectSettingsEditorPart"
            default="true"
            filenames="modembed.project"
            icon="icons/project_settings.png"
            id="hu.cubussapiens.modembed.ui.project.settingsEditor"
            name="ProjectSettingsEditor">
      </editor>
   </extension>
   <extension
         id="masmbuilder"
         point="org.eclipse.core.resources.builders">
      <builder
            callOnEmptyDelta="true"
            hasNature="true"
            isConfigurable="false">
         <run
               class="hu.cubussapiens.modembed.ui.builder.MASMBuilder">
         </run>
      </builder>
   </extension>
   <extension
         id="marker"
         point="org.eclipse.core.resources.markers">
      <persistent
            value="true">
      </persistent>
      <super
            type="org.eclipse.core.resources.problemmarker">
      </super>
      <super
            type="org.eclipse.core.resources.textmarker">
      </super>
   </extension>

</plugin>
