<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.importWizards">
      <wizard
            category="hu.modembed.ui.category"
            class="hu.modembed.pic.ui.wizard.ImportPICLibWizard"
            icon="icons/microchip.png"
            id="hu.modembed.pic.ui.importLib"
            name="Import PIC library">
      </wizard>
   </extension>
   <extension
         point="hu.modembed.ui.includedCode">
      <project
            category="hu.modembed.pic.ui.category"
            icon="icons/microchip.png"
            id="microchip.pic16"
            name="PIC16 midrange architecture code">
         <dependency
               project="e.core">
         </dependency>
         <resource
               path="/"
               resource="code/pic16/pic16.e">
         </resource>
         <resource
               path="/"
               resource="code/pic16/pic16e.e">
         </resource>
         <resource
               path="/"
               resource="code/pic16/pic16.platform.e">
         </resource>
      </project>
      <category
            id="hu.modembed.pic.ui.category"
            name="PIC">
      </category>
   </extension>

</plugin>
