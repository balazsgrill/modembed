		<super type="org.eclipse.core.resources.problemmarker">
		</super>
		<super type="org.eclipse.core.resources.textmarker">
		</super>
		<super type="org.eclipse.emf.ecore.diagnostic">
		</super>
	</extension>
	
	<extension id="hu.modembed.model.textnotations.type.problem.syntax_error" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="hu.modembed.model.textnotations.type.problem">
		</super>
	</extension>
	
	<extension id="hu.modembed.model.textnotations.type.problem.print_problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="hu.modembed.model.textnotations.type.problem">
		</super>
	</extension>
	
	<extension id="hu.modembed.model.textnotations.type.problem.unresolved_reference" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="hu.modembed.model.textnotations.type.problem">
		</super>
	</extension>
	
	<extension id="hu.modembed.model.textnotations.type.problem.analysis_problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
