<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension point="org.emftext.access.syntax">
		<metaInformationProvider class="hu.modembed.model.textnotations.type.mopp.TypeMetaInformation" id="hu.modembed.model.textnotations.type.mopp.TypeMetaInformation">
		</metaInformationProvider>
	</extension>
	
	<extension id="hu.modembed.model.textnotations.type.problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
