<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension point="org.emftext.access.syntax">
		<metaInformationProvider class="hu.modembed.model.textnotations.device.mopp.DeviceMetaInformation" id="hu.modembed.model.textnotations.device.mopp.DeviceMetaInformation">
		</metaInformationProvider>
	</extension>
	
	<extension id="hu.modembed.model.textnotations.device.problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.eclipse.core.resources.problemmarker">
		</super>
		<super type="org.eclipse.core.resources.textmarker">
		</super>
		<super type="org.eclipse.emf.ecore.diagnostic">
		</super>
	</extension>
	
	<extension id="hu.modembed.model.textnotations.device.problem.syntax_error" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="hu.modembed.model.textnotations.device.problem">
		</super>
	</extension>
	
	<extension id="hu.modembed.model.textnotations.device.problem.print_problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="hu.modembed.model.textnotations.device.problem">
		</super>
	</extension>
	
	<extension id="hu.modembed.model.textnotations.device.problem.unresolved_reference" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="hu.modembed.model.textnotations.device.problem">
		</super>
	</extension>
	
	<extension id="hu.modembed.model.textnotations.device.problem.analysis_problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="hu.modembed.model.textnotations.device.problem">
		</super>
	</extension>
	
	<extension id="hu.modembed.model.textnotations.device.problem.batch_constraint_problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="hu.modembed.model.textnotations.device.problem">
		</super>
	</extension>
	
	<extension id="hu.modembed.model.textnotations.device.problem.live_constraint_problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="hu.modembed.model.textnotations.device.problem">
		</super>
	</extension>
	
	<extension id="hu.modembed.model.textnotations.device.problem.builder_error" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="hu.modembed.model.textnotations.device.problem">
		</super>
	</extension>
	
	<extension id="hu.modembed.model.textnotations.device.nature" name="device nature" point="org.eclipse.core.resources.natures">
		<runtime>
			<run class="hu.modembed.model.textnotations.device.mopp.DeviceNature">
			</run>
		</runtime>
		<builder id="hu.modembed.model.textnotations.device.builder">
		</builder>
	</extension>
	
	<extension point="org.eclipse.core.resources.builders" id="hu.modembed.model.textnotations.device.builder" name="device Builder">
		<builder hasNature="true">
			<run class="hu.modembed.model.textnotations.device.mopp.DeviceBuilderAdapter">
			</run>
		</builder>
	</extension>
	
	<extension-point id="hu.modembed.model.textnotations.device.default_load_options" name="Default Load Options" schema="schema/default_load_options.exsd">
	</extension-point>
	
	<extension point="org.eclipse.emf.ecore.extension_parser">
		<parser class="hu.modembed.model.textnotations.device.mopp.DeviceResourceFactoryDelegator" type="device">
		</parser>
	</extension>
	
	<extension-point id="hu.modembed.model.textnotations.device.additional_extension_parser" name="Additional Extension Parser" schema="schema/additional_extension_parser.exsd">
	</extension-point>
	
</plugin>
