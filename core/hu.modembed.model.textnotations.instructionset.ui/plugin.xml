			<description description="Run instructionset model" mode="run">
			</description>
			<description description="Debug instructionset model" mode="debug">
			</description>
			<contextualLaunch>
				<enablement>
					<with variable="selection">
						<count value="1">
						</count>
						<iterate>
							<test property="hu.modembed.model.modembed.core.instructionset.isLaunchable">
							</test>
						</iterate>
					</with>
				</enablement>
			</contextualLaunch>
