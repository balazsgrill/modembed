<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="hu.modembed.programmers" name="Programmers" schema="schema/hu.modembed.programmers.exsd"/>
   <extension-point id="hu.modembed.ui.includedCode" name="Included Code" schema="schema/hu.modembed.ui.includedCode.exsd"/>
   <extension
         point="hu.modembed.ui.includedCode">
      <category
            id="hu.modembed.category.core"
            name="Generic">
      </category>
      <project
            archive="shared/e.core.zip"
            category="hu.modembed.category.core"
            id="e.core"
            name="Core resources">
      </project>
   </extension>
   <extension
         point="org.eclipse.ant.core.antTasks">
      <antTask
            class="hu.modembed.ant.AssembleTask"
            headless="true"
            library="ant_tasks/modembed.ant.tasks.jar"
            name="modembed.assemble">
      </antTask>
      <antTask
            class="hu.modembed.ant.DisassembleTask"
            headless="true"
            library="ant_tasks/modembed.ant.tasks.jar"
            name="modembed.disassemble">
      </antTask>
      <antTask
            class="hu.modembed.ant.ModuleLinkerTask"
            eclipseRuntime="true"
            headless="true"
            library="ant_tasks/modembed.ant.tasks.jar"
            name="modembed.link">
      </antTask>
      <antTask
            class="hu.modembed.ant.hex.MergeHexFilesTask"
            headless="true"
            library="ant_tasks/modembed.ant.tasks.jar"
            name="modembed.hex.merge">
      </antTask>
      <antTask
            class="hu.modembed.ant.hex.ExtractHexFileTask"
            headless="true"
            library="ant_tasks/modembed.ant.tasks.jar"
            name="modembed.hex.extract">
      </antTask>
      <antTask
            class="hu.modembed.ant.TranslateTask"
            eclipseRuntime="true"
            headless="true"
            library="ant_tasks/modembed.ant.tasks.jar"
            name="modembed.translate">
      </antTask>
      <antTask
            class="hu.modembed.ant.MapTask"
            eclipseRuntime="true"
            headless="true"
            library="ant_tasks/modembed.ant.tasks.jar"
            name="modembed.map">
      </antTask>
      <antTask
            class="hu.modembed.ant.ModuleCompilerTask"
            eclipseRuntime="true"
            headless="true"
            library="ant_tasks/modembed.ant.tasks.jar"
            name="modembed.compile">
      </antTask>
   </extension>
   <extension
         point="org.eclipse.ant.core.extraClasspathEntries">
      <extraClasspathEntry
            eclipseRuntime="true"
            headless="true"
            library="ant_tasks/modembed.ant.tasks.jar">
      </extraClasspathEntry>
   </extension>
   <extension
         point="hu.textualmodeler.grammars">
      <grammar
            id="modembed.basic.operators"
            model="grammars/modembed.basic.operators.grammar">
      </grammar>
      <grammar
            id="modembed.device"
            model="grammars/modembed.device.grammar">
      </grammar>
      <grammar
            id="modembed.expressions"
            model="grammars/modembed.expressions.grammar">
      </grammar>
      <grammar
            id="modembed"
            model="grammars/modembed.grammar">
      </grammar>
      <grammar
            id="modembed.instructionSet"
            model="grammars/modembed.instructionSet.grammar">
      </grammar>
      <grammar
            id="modembed.mappingrules"
            model="grammars/modembed.mappingrules.grammar">
      </grammar>
      <grammar
            id="modembed.module"
            model="grammars/modembed.module.grammar">
      </grammar>
      <grammar
            id="modembed.sequential"
            model="grammars/modembed.sequential.grammar">
      </grammar>
      <grammar
            id="modembed.type"
            model="grammars/modembed.type.grammar">
      </grammar>
   </extension>
   <extension
         point="org.eclipse.emf.ecore.extension_parser">
      <parser
            class="hu.modembed.resource.MResourceFactory"
            type="m">
      </parser>
   </extension>

</plugin>
