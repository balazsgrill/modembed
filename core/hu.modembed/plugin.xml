<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="hu.modembed.programmers" name="Programmers" schema="schema/hu.modembed.programmers.exsd"/>
   <extension-point id="hu.modembed.ui.includedCode" name="Included Code" schema="schema/hu.modembed.ui.includedCode.exsd"/>
   <extension
         point="hu.modembed.ui.includedCode">
      <category
            id="hu.modembed.category.core"
            name="Generic">
      </category>
      <project
            archive="shared/e.core.zip"
            category="hu.modembed.category.core"
            id="e.core"
            name="Core resources">
      </project>
   </extension>
   <extension
         point="org.eclipse.ant.core.antTasks">
      <antTask
            class="hu.modembed.ant.AssembleTask"
            headless="true"
            library="ant_tasks/modembed.ant.tasks.jar"
            name="modembed.assemble">
      </antTask>
      <antTask
            class="hu.modembed.ant.DisassembleTask"
            headless="true"
            library="ant_tasks/modembed.ant.tasks.jar"
            name="modembed.disassemble">
      </antTask>
      <antTask
            class="hu.modembed.ant.ModuleCompilerTask"
            eclipseRuntime="true"
            headless="true"
            library="ant_tasks/modembed.ant.tasks.jar"
            name="modembed.compile">
      </antTask>
   </extension>
   <extension
         point="org.eclipse.ant.core.extraClasspathEntries">
      <extraClasspathEntry
            eclipseRuntime="true"
            headless="true"
            library="ant_tasks/modembed.ant.tasks.jar">
      </extraClasspathEntry>
   </extension>

</plugin>
