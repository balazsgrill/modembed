# Internal Program Memory (128K bytes)
pgmmem (region=0x1D000000-0x1D01FFFF kuseg=0x0 kseg0=0x80000000 kseg1=0xA0000000 kseg2_3=0x0)

# Internal Boot Config Memory (12K bytes minus 16 bytes)
