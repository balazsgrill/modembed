<?xml version="1.0" encoding="ASCII"?>
<pic:PicCPUType xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:configscheme="http://gb.hu/embedded/configuration/scheme" xmlns:pic="http://gb.hu/embedded/pic" name="PIC12F510">
  <configuration wordsize="8">
    <scheme>
      <subsets name="CONFIG">
        <parameters xsi:type="configscheme:EnumParam" name="Oscillator Select">
          <literals name="EXTRC With 1 ms DRT" value="3"/>
          <literals name="INTRC With 1 ms DRT" value="2"/>
          <literals name="XT Osc With 18 ms DRT" value="1"/>
          <literals name="LP Osc With 18 ms DRT"/>
        </parameters>
        <parameters xsi:type="configscheme:EnumParam" name="Watchdog Timer Enable">
          <literals name="Enabled" value="1"/>
          <literals name="Disabled"/>
        </parameters>
        <parameters xsi:type="configscheme:EnumParam" name="Code Protect">
          <literals name="Off" value="1"/>
          <literals name="On"/>
        </parameters>
        <parameters xsi:type="configscheme:EnumParam" name="Master Clear Enable">
          <literals name="GP3/MCLR Functions as MCLR" value="1"/>
          <literals name="GP3/MCLR Functions as GP3"/>
        </parameters>
        <parameters xsi:type="configscheme:EnumParam" name="Internal Oscillator Frequency Select">
          <literals name="8 MHz INTOSC Speed" value="1"/>
          <literals name="4 MHz INTOSC Speed"/>
        </parameters>
      </subsets>
    </scheme>
    <words address="4095">
      <fields parameter="//@configuration/@scheme/@subsets.0/@parameters.0" length="2"/>
      <fields parameter="//@configuration/@scheme/@subsets.0/@parameters.1" startBit="2" length="1"/>
      <fields parameter="//@configuration/@scheme/@subsets.0/@parameters.2" startBit="3" length="1"/>
      <fields parameter="//@configuration/@scheme/@subsets.0/@parameters.3" startBit="4" length="1"/>
      <fields parameter="//@configuration/@scheme/@subsets.0/@parameters.4" startBit="5" length="1"/>
    </words>
  </configuration>
</pic:PicCPUType>
