<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="MODembed"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.eclipse.ui.ide.workbench"
            name="MODembed">
         <property
               name="windowImages"
               value="icons/window/cpu_16.png,icons/window/cpu_48.png">
         </property>
         <property
               name="aboutText"
               value="MODembed {0} ({1}/{2})&#x0A;&#x0A;Model-based embedded development environment&#x0A;&#x0A;web page: http://balazsgrill.github.io/modembed/">
         </property>
         <property
               name="aboutImage"
               value="icons/window/cpu_50.png">
         </property>
         <property
               name="appName"
               value="MODembed">
         </property>
         <property
               name="preferenceCustomization"
               value="plugin_customization.ini">
         </property>
      </product>
   </extension>

</plugin>
