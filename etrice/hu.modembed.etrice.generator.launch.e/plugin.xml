<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.debug.core.launchConfigurationTypes">
      <launchConfigurationType
            delegate="hu.modembed.etrice.generator.e.EGeneratorLaunchConfigDelegate"
            delegateName="eTrice e Generator"
            id="hu.modembed.etrice.generator.e.launchConfigurationType"
            modes="run, debug"
            name="eTrice e generator">
      </launchConfigurationType>
   </extension>
   <extension
         point="org.eclipse.debug.ui.launchConfigurationTabGroups">
      <launchConfigurationTabGroup
            class="hu.modembed.etrice.generator.e.EGeneratorLaunchConfigurationTabGroup"
            description="e Generator configuration tab group"
            id="hu.modembed.etrice.generator.launch.e.launchConfigurationTabGroup"
            type="hu.modembed.etrice.generator.e.launchConfigurationType">
      </launchConfigurationTabGroup>
   </extension>
   <extension
         point="org.eclipse.debug.ui.launchConfigurationTypeImages">
      <launchConfigurationTypeImage
            configTypeID="hu.modembed.etrice.generator.e.launchConfigurationType"
            icon="icons/efile.gif"
            id="hu.modembed.etrice.generator.launch.e.launchConfigurationTypeImage">
      </launchConfigurationTypeImage>
   </extension>

</plugin>
